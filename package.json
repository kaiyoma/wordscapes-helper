{
  "name": "wordscapes-helper",
  "version": "0.1.0",
  "description": "Command-line tool to offer suggestions for Wordscapes puzzles",
  "author": "kyle.getz@gmail.com",
  "homepage": "http://www.kylegetz.me",
  "license": "MIT",
  "scripts": {
    "build": "bash build.sh",
    "build:docs": "typedoc",
    "build:tool": "babel src --out-dir dist --extensions .ts",
    "clean": "rimraf .jest-cache *.log coverage dist docs",
    "find-words": "node dist/find-words.js src/words.txt",
    "lint": "eslint --max-warnings 0 --ext .js,.ts *.js src",
    "prettier": "prettier --check *.js *.json src",
    "test": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cli-progress": "3.10.0"
  },
  "devDependencies": {
    "@arista/prettier-config": "1.1.4",
    "@babel/cli": "7.17.3",
    "@babel/core": "7.17.5",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/cli-progress": "3.9.2",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "babel-jest": "27.5.1",
    "eslint": "8.9.0",
    "eslint-config-arista-js": "2.0.2",
    "eslint-config-arista-ts": "2.0.1",
    "eslint-plugin-arista": "0.2.3",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "jest-standard-reporter": "2.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "tslib": "2.3.1",
    "typedoc": "0.22.11",
    "typescript": "4.5.5"
  }
}
